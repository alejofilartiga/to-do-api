{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger-ui/(.*)",
            "dest": "/node_modules/swagger-ui-dist/$1"
        },
        {
            "src": "/swagger-ui.css",
            "dest": "/node_modules/swagger-ui-dist/swagger-ui.css"
        },
        {
            "src": "/swagger-ui-bundle.js",
            "dest": "/node_modules/swagger-ui-dist/swagger-ui-bundle.js"
        },
        {
            "src": "/swagger-ui-standalone-preset.js",
            "dest": "/node_modules/swagger-ui-dist/swagger-ui-standalone-preset.js"
        },
        {
            "src": "/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "https://to-do-alejo.netlify.app",
                "Access-Control-Allow-Methods": "GET, POST, PATCH, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization",
                "Access-Control-Allow-Credentials": "true"
            },
            "dest": "index.ts"
        }
    ]
}